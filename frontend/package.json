{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o build/static/css/tailwind.css --config tailwind.config.mjs",  
