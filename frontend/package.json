{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o build/static/css/tailwind.css --config tailwind.config.mjs",
    "build:sass": "sass src/styles/styles.scss build/static/css/styles.css",
    "build-combo": "npm run build:css && npm run build:sass",
    "watch": "npm run build-combo -- --watch",
    "tester": "jest",
    "lint": "eslint 'src/**/*.{js,jsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,jsx}' --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "channels": "^0.0.4",
    "cheerio": "^1.0.0-rc.12",
    "flake8": "^0.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.77.4",
    "tailwindcss": "^3.4.3",
    "three": "^0.165.0",
    "web-vitals": "^4.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.24.6",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@rollup/plugin-terser": "0.4.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "sass-loader": "^14.2.1"
  },
  "overrides": {
    "css-select": {
      "nth-check": "^2.0.1"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
