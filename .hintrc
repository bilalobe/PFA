{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "html-has-lang": "off",
    "typescript-config/consistent-casing": "on",
    "typescript-config/no-useless-files": "on",
    "typescript-config/strict-boolean-expressions": "off",
    "typescript-config/strict-type-assertions": "off"
  }
}
